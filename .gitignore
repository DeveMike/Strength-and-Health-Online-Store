# WordPress core files (ignore these)
wp-admin/
wp-includes/

# Configuration file (it may have sensitive data)
wp-config.php

# Ignore plugin and theme cache
wp-content/cache/
wp-content/upgrade/

# Uploads: if you want to include images, leave this uncommented
#wp-content/uploads/

# Ignore backups and temporary files
ai1wm-backups/
backups-dup-lite/

